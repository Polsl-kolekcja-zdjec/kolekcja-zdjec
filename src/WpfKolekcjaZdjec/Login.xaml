        <telerik:RadButton Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="167,113,0,0" Name="Cancel" VerticalAlignment="Top" Width="78" Click="Cancel_Click" />
        <Label Content="Login" Height="28" HorizontalAlignment="Left" Margin="27,31,0,0" Name="label1" VerticalAlignment="Top" Foreground="#E6000000" OpacityMask="Black" FontWeight="Bold" />
        <TextBox Background="#9B2883C3" Height="23" HorizontalAlignment="Left" Margin="68,35,0,0" Name="textBox2" VerticalAlignment="Top" Width="177" MaxLength="20" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="68,67,0,0" Name="passwordBox1" VerticalAlignment="Top" Width="177" Background="#9B2883C3" />
