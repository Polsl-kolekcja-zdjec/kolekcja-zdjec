        <Rectangle Height="200" HorizontalAlignment="Left" Name="rectangle1" Stroke="Black" VerticalAlignment="Top" Width="300" Fill="White" OpacityMask="#71000000" Margin="-5,-4,0,0" />
        <Label Content="Hasło" Height="28" HorizontalAlignment="Left" Margin="25,65,0,0" Name="label2" VerticalAlignment="Top" Foreground="#E6110B0B" FontWeight="Bold" />
